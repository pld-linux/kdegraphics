     return;
@@ -36,8 +42,21 @@
   proc.readln(map_fileName);
   map_fileName = map_fileName.stripWhiteSpace();
   if (map_fileName.isEmpty()) {
-    kdError(4700) << "fontMap::fontMap(): The file 'ps2pk.map' could not be found by kpsewhich." << endl;
