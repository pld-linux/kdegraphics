+						<keycombo action="simul">&Ctrl;<keycap>Home</keycap></keycombo> or
+						using 
+						<menuchoice>
+							<guimenu>Go</guimenu>
+							<guimenuitem>First Page</guimenuitem>
+						</menuchoice>.
+					</para>
+				</listitem>
+				<listitem>
+					<para>
