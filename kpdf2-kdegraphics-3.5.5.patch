+void KPDFPage::removeAllBookmarks()
+{
+    QValueList< KPDFBookmark * >::iterator it = m_bookmarks.begin(), end = m_bookmarks.end();
+    while ( it != end )
+    {
+        KPDFBookmark* bm = *it;
+        it = m_bookmarks.remove(it);
+        delete bm;
+    }
