+    QValueList< KPDFBookmark * >::iterator it = m_bookmarks.begin(), end = m_bookmarks.end();
+    while ( it != end )
+    {
+        KPDFBookmark* bm = *it;
+        QRect rect = bm->Rect();
+        int x = point.x(), y = point.y();
+        if( x > rect.left() && x < rect.right() && y > rect.top() && y < rect.bottom() )
+        {
+            it = m_bookmarks.remove(it);
+            break;
+        }
+        ++it;
+    }
+}
